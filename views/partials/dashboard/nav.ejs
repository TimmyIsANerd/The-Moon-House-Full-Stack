<% const activeLinkHeader={ header:'' };
  if(activeNavLink=='dashboard' ){
    activeLinkHeader.header=`Welcome
  ${user.firstName}`;
  } else if(activeNavLink=='profile' ){
    activeLinkHeader.header='User Profile (KYC Information)';
  } else if(activeNavLink=='security' ){
    activeLinkHeader.header='User Security';
  } else if(activeNavLink=='notifications' ){
    activeLinkHeader.header='Notifcations';
  } else if(activeNavLink=='history' ){
    activeLinkHeader.header='Transaction History';
  } %>
  <!-- Navbar -->
  <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top " id="navigation-example">
    <div class="container-fluid">
      <div class="navbar-wrapper">
        <a class="navbar-brand" href="javascript:void(0)">
          <%= activeLinkHeader.header %>
        </a>
      </div>
      <button class="navbar-toggler" type="button" data-toggle="collapse" aria-controls="navigation-index"
        aria-expanded="false" aria-label="Toggle navigation" data-target="#navigation-example">
        <span class="sr-only">Toggle navigation</span>
        <span class="navbar-toggler-icon icon-bar"></span>
        <span class="navbar-toggler-icon icon-bar"></span>
        <span class="navbar-toggler-icon icon-bar"></span>
      </button>
      <div class="collapse navbar-collapse justify-content-end">
        <ul class="navbar-nav">
          <li class="nav-item dropdown">
            <a class="nav-link" href="javscript:void(0)" id="navbarDropdownMenuLink" data-toggle="dropdown"
              aria-haspopup="true" aria-expanded="false">
              <i class="material-icons">notifications</i>
                  <span class="notification">
                    <% if (user.userNotifications && user.userNotifications[0] && user.userNotifications[0].notification) { %>
                      <%= user.userNotifications[0].notification.length %>
                    <% } else { %>
                      0
                    <% } %>
                  </span>
                  <p class="d-lg-none d-md-block">
                    Notifications
                  </p>
            </a>
            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
              <% if (user.userNotifications && user.userNotifications[0] && user.userNotifications[0].notification) { %>
                <% user.userNotifications[0].notification.forEach(notification=> {%>

                  <a class="dropdown-item" href="javascript:void(0)">
                    <%= notification.message %>
                  </a>

                  <% }); %>
              <% } %>
            </div>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  <!-- End Navbar -->